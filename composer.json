{
    "name": "thethree/myfatoorah",
    "description": "My fatoorah api wrapper ",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ayman Elmalah",
            "email": "aymanelmalah77@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
    },
    "autoload": {
        "psr-4": {
            "AymanElmalah\\MyFatoorah\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AymanElmalah\\MyFatoorah\\MyFatoorahServiceProvider"
            ],
            "aliases": {
                "MyFatoorah": "AymanElmalah\\MyFatoorah\\Facades\\MyFatoorah"
            }
        }
    }
}
